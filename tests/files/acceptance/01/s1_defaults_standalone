## Ansible managed

## Defaults specification
Defaults compress_io
Defaults iolog_dir = /var/log/sudo-io
Defaults@thost !authenticate
Defaults:tuser !authenticate
Defaults!/bin/test !authenticate
Defaults>toperator !authenticate
Defaults!/bin/test timestamp_timeout = 30
Defaults>toperator timestamp_timeout = 30
Defaults@thost1,thost2,thost3 insults
Defaults:tuser1,tuser2,tuser3 insults
Defaults!/bin/test1,/bin/test2,/bin/test3 insults
Defaults>toperator1,toperator2,toperator3 insults
Defaults:tuser !visiblepw
Defaults@thost !visiblepw
Defaults>toperator !visiblepw
Defaults!/bin/test !visiblepw
